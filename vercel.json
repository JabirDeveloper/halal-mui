{
    "version": 2,
    "builds": [
        { "src": "./public/**", "use": "@vercel/static" },
        { "src": "./bin/**", "use": "@vercel/static" },
        { "src": "./routes/**", "use": "@vercel/static" },
        { "src": "./*.md", "use": "@vercel/node" },
        { "src": "./*.js", "use": "@vercel/node" }
    ],
    "routes": [
     {
         "src": "/(.*)",
         "dest": "/"
     }
    ]
 }